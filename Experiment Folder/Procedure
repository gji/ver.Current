Window Table0() : Table
	PauseUpdate; Silent 1		// building window...
	String fldrSav0= GetDataFolder(1)
	SetDataFolder root:PTS_Parameters:
	Edit/W=(5.25,42.5,510,236.75) PTS_Param
	ModifyTable format(Point)=1
	SetDataFolder fldrSav0
EndMacro


